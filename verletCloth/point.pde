class point{
  float x, y, oldx, oldy, gravity, friction;
  boolean isStatic;
  
  point(float x_, float y_, float oldx_, float oldy_, float gravity_, float friction_, boolean isStatic_){
    x = x_;
    y = y_;
    oldx = oldx_;
    oldy = oldy_;
    gravity = gravity_;
    friction = friction_;
    isStatic = isStatic_;
  }
  
  
}
